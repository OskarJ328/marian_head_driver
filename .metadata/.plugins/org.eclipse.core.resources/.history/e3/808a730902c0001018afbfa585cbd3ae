#include "gpio.h"

#define TIM3_CH1_PIN 2



#define PA6_AF GPIOA->MODER |= (0b10 << (6*TIM3_CH1_PIN))
#define PA6_HS GPIOA->OSPEEDR |= (0b10 << (2*TIM3_CH1_PIN))
#define PA6_TIM3 GPIOA->AFR[0] |= (2 << (4*TIM3_CH1_PIN))


static void gpioa_tim3_setup(void){
	PA6_AF;
	PA6_HS;
	PA6_TIM3;
}
void gpioa_setup(void){
	gpioa_tim3_setup();
}


